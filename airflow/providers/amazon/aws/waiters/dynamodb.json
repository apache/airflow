{
    "version": 2,
    "waiters": {
        "export_table": {
            "operation": "DescribeExport",
            "delay": 30,
            "maxAttempts": 60,
            "acceptors": [
                {
                    "matcher": "path",
                    "expected": "COMPLETED",
                    "argument": "exportdescription.exportstatus",
                    "state": "success"
                },
                {
                    "matcher": "path",
                    "expected": "FAILED",
                    "argument": "exportdescription.exportstatus",
                    "state": "failure"
                },
                {
                    "matcher": "path",
                    "expected": "IN_PROGRESS",
                    "argument": "exportdescription.exportstatus",
                    "state": "retry"
                }
            ]
        }
    }
}
