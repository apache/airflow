        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.19",
        "eslint-plugin-unicorn": "^55.0.0",
        "happy-dom": "^17.4.4",
        "prettier": "^3.5.3",
        "typescript-eslint": "^8.27.0",
        "vite": "^6.2.3",
        "vite-plugin-css-injected-by-js": "^3.5.2",
        "vitest": "^3.0.9"
