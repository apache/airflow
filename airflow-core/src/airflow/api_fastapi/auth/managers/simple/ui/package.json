        "react": "^19.0.0",
        "react-cookie": "^7.2.2",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.54.2",
        "react-router-dom": "^7.4.0",
        "react@18.3.19": "link:types/react@18.3.19"
    },
    "devDependencies": {
        "@7nohe/openapi-react-query-codegen": "^1.6.2",
        "@eslint/compat": "^1.2.7",
        "@eslint/js": "^9.23.0",
        "@stylistic/eslint-plugin": "^2.13.0",
        "@testing-library/jest-dom": "^6.6.3",
