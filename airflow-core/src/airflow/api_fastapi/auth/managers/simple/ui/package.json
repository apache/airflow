{
    "name": "simple-auth-manager-ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --port 5174",
        "build": "vite build",
        "lint": "eslint --quiet && tsc --p tsconfig.app.json",
